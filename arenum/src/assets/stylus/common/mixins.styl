// font-face
webfont($family, $file, $weight = normal, $style = normal) {
  @font-face {
    font-family: $family;
    src: url('../fonts/' + $file + '.eot');
    src: url('../fonts/' + $file + '.eot?#iefix') format('embedded-opentype'), url('../fonts/' + $file + '.woff') format('woff'), url('../fonts/' + $file + '.ttf') format('truetype');
    font-weight: $weight;
    font-style: $style;
  }
}